<%- include('partials/html-head') %> <%- include('partials/html-navbar') %>
<div class="container mx-auto mt-4">
  <table>
    <% for(let c=0; c<16; c++){ %>
      <tr>
        <% for(let d=0; d<16; d++){ %>
        <td class="w-12 h-12" style="background-color: #<%= c.toString(16) %>0<%= d.toString(16) %>"></td>
        <% } %>
      </tr>
    <% } %>
    </tbody>
  </table>
  <table>
    <% for(let c=0; c<16; c++){ %>
      <tr>
        <% for(let d=0; d<16; d++){ %>
        <td class="w-12 h-12" style="background-color: #<%= Math.floor(Math.random()*(2**24)).toString(16).padStart(6, '0') %>"></td>
        <% } %>
      </tr>
    <% } %>
    </tbody>
  </table>
</div>

<%- include('partials/html-scripts') %> <%- include('partials/html-tail') %>
